<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <RootNamespace>prometheus_monitor</RootNamespace>
    <Nullable>enable</Nullable>
	<Description>Prometheus-Monitor is a lightweight C# .NET package for tracking unusual behavior on endpoints.</Description>
	<PackageTags>behavioral analysis; endpoint security; login monitoring; IP tracking; .NET</PackageTags>
	<Authors>Frank Mensah</Authors>
	<Company>Frank Mensah</Company>
	<PackageId>menfra.prometheus-monitor</PackageId>
	<PackageReadmeFile>README.md</PackageReadmeFile>
	<Version>3.0.3</Version>
	<PackageLicenseExpression>MIT</PackageLicenseExpression>
	<RepositoryUrl>https://github.com/menfra/prometheus-monitor</RepositoryUrl>
	<RepositoryType>git</RepositoryType>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.DependencyInjection" Version="9.0.0" />
	<None Include="..\README.md" Pack="true" PackagePath="\"/>
  </ItemGroup>
</Project>
